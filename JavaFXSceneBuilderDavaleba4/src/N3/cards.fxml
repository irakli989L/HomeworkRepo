<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="N3.CardsController">
   <children>
      <CheckBox fx:id="balloonsCheck" layoutX="269.0" layoutY="187.0" mnemonicParsing="false" prefHeight="118.0" prefWidth="232.0" text="Balloons">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </CheckBox>
      <Label alignment="CENTER" layoutX="115.0" layoutY="40.0" prefHeight="105.0" prefWidth="771.0" text="Select a Theme For Your Card">
         <font>
            <Font name="System Bold Italic" size="40.0" />
         </font>
      </Label>
      <CheckBox fx:id="heartsCheck" layoutX="269.0" layoutY="305.0" mnemonicParsing="false" prefHeight="118.0" prefWidth="232.0" text="Hearts">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="starsCheck" layoutX="269.0" layoutY="423.0" mnemonicParsing="false" prefHeight="118.0" prefWidth="232.0" text="Stars">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </CheckBox>
      <Button fx:id="generateButton" layoutX="390.0" layoutY="610.0" mnemonicParsing="false" onAction="#generateCard" prefHeight="85.0" prefWidth="221.0" text="Generate">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
